{
  "name": "selenium-cucumber",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "API-REPORTER": "node_modules/.bin/selenium-standalone start & node features/commons/httpConfig.js --env=$ENV | node_modules/.bin/cucumber-js --tags=$TAGS --format=json:report/cucumber_report.json -f @cucumber/pretty-formatter features/* || node index.js",
    "API": "node commons/httpConfig.js --env=$ENV | node_modules/.bin/cucumber-js features/api/* --require stepDefinitions/ --format=json:reports/API/cucumber_report.json -f @cucumber/pretty-formatter",
    "UI": "node commons/httpConfig.js --env=$ENV && DEBUG=true wdio ./wdio_confs/wdio.local.conf.js",
    "UI-REMOTE": "node commons/httpConfig.js --env $ENV && wdio ${npm_config_conf} --cucumberOpts.retry 2",
    "UI-VISUAL": "node commons/httpConfig.js --env $ENV && wdio ${npm_config_conf}",
    "allure-report": "node_modules/.bin/allure generate --clean ./reports/UI/allure-results/ && node_modules/.bin/allure open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "allure-cucumberjs": "^2.0.0-beta.13",
    "chromedriver": "^100.0.0",
    "concurrently": "^6.3.0",
    "cucumberjs-chromedriver": "^0.3.1",
    "express": "^4.17.1",
    "form-data": "^4.0.0",
    "fs": "0.0.1-security",
    "fs-extra": "^10.0.0",
    "kill-port-process": "^3.0.1",
    "log4js": "^6.3.0",
    "moment": "^2.29.1",
    "node-sessionstorage": "^1.0.0",
    "querystring": "^0.2.1",
    "selenium-webdriver": "^4.1.1",
    "sinon-chai": "^3.7.0",
    "superagent-charset": "^1.2.0",
    "superagent-retry-delay": "^2.6.5",
    "xlsx": "^0.18.0",
    "xlsx-parse-stream": "^1.0.3",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.0",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.1",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "@wdio/allure-reporter": "^7.7.6",
    "@wdio/cli": "^7.19.7",
    "@wdio/codemod": "^0.9.1",
    "@wdio/cucumber-framework": "^7.19.7",
    "@wdio/local-runner": "^7.19.7",
    "@wdio/selenium-standalone-service": "^7.19.5",
    "@wdio/spec-reporter": "^7.19.7",
    "allure-commandline": "^2.13.0",
    "chai": "^4.3.4",
    "cucumber-html-reporter": "^5.4.0",
    "eslint": "^8.4.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-xo": "^0.37.0",
    "eslint-plugin-cucumber": "^1.4.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-wdio": "^7.4.2",
    "selenium-standalone": "^8.0.4",
    "supertest": "^6.1.6",
    "wdio-chromedriver-service": "^7.2.0",
    "wdio-image-comparison-service": "^2.3.0",
    "wdio-video-reporter": "^3.2.1",
    "webdriverio": "^7.19.1"
  }
}
